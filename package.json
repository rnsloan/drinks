{
  "name": "drinks",
  "version": "1.0.0",
  "main": "index.js",
  "author": "rnsloan1@gmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development parcel index.html -p 3000 --hmr-port 45880",
    "build": "NODE_ENV=production parcel build index.html --public-url ./",
    "test": "jest",
    "e2e": "node ./e2e/index.js"
  },
  "browserslist": [
    "since 2019-01"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.3",
    "@types/aphrodite": "^0.5.13",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "puppeteer": "^1.12.2",
    "react-test-renderer": "^16.8.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "aphrodite": "^2.2.3",
    "firebase": "^5.8.2",
    "query-string": "^6.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-router5": "^7.0.2",
    "router5": "^7.0.1",
    "router5-plugin-browser": "^7.0.1",
    "router5-plugin-listeners": "^7.0.1",
    "router5-plugin-logger": "^7.0.1",
    "use-async-effect": "^2.0.0"
  }
}
